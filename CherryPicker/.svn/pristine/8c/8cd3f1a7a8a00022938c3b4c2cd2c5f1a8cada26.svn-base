	select mem_name,mem_num,p_word,use_ip
		  ,mem_level,mem_work,mem_part
		  ,sec_level,work_yn,manage_level
		  ,work_type,CONVERT(CHAR(19), in_date, 20) as in_date
		  ,CONVERT(CHAR(19), mod_date, 20) as mod_date
